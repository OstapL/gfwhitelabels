- var config = require('./config.js');
doctype html
html(lang="en")
  head
    title #{ htmlWebpackPlugin.options.title }
    include ga.script.pug
    meta(
      name="description"
      content="GrowthFountain is an SEC registered Funding Portal and FINRA Member where individuals can come together to support entrepreneurs and small businesses across America. Turn your customers, your social network, your neighbors and community members into owners who can become your brand ambassadors and evangelists. To learn more about GrowthFountain and the risks of crowdfunding, please visit www.growthfountain.com"
    )
    meta(charset="utf-8")
    //meta(name="author" content="Growthfountain")
    <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'><![endif]-->
    meta(name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0")
    <!-- Favicons -->
    link(rel="shortcut icon" href=require('images/favicon.ico') type="image/x-icon")
    style.
      .loader_overlay{position:fixed;width:100%;height:100%;background:#fff;z-index:9999}.loader_overlay .loader{width:50px;height:50px;position:absolute;top:50%;left:50%;margin:-25px 0 0 -25px;font-size:10px;text-indent:-12345px;border-top:1px solid rgba(0,0,0,.08);border-right:1px solid rgba(0,0,0,.08);border-bottom:1px solid rgba(0,0,0,.08);border-left:1px solid rgba(0,0,0,.5);-webkit-border-radius:50%;-moz-border-radius:50%;border-radius:50%;-webkit-animation:spinner .7s infinite linear;-moz-animation:spinner .7s infinite linear;-ms-animation:spinner .7s infinite linear;-o-animation:spinner .7s infinite linear;animation:spinner .7s infinite linear;z-index:100001}@-webkit-keyframes spinner{0%{-webkit-transform:rotate(0);-moz-transform:rotate(0);-ms-transform:rotate(0);-o-transform:rotate(0);transform:rotate(0)}100%{-webkit-transform:rotate(360deg);-moz-transform:rotate(360deg);-ms-transform:rotate(360deg);-o-transform:rotate(360deg);transform:rotate(360deg)}}@-moz-keyframes spinner{0%{-webkit-transform:rotate(0);-moz-transform:rotate(0);-ms-transform:rotate(0);-o-transform:rotate(0);transform:rotate(0)}100%{-webkit-transform:rotate(360deg);-moz-transform:rotate(360deg);-ms-transform:rotate(360deg);-o-transform:rotate(360deg);transform:rotate(360deg)}}@-o-keyframes spinner{0%{-webkit-transform:rotate(0);-moz-transform:rotate(0);-ms-transform:rotate(0);-o-transform:rotate(0);transform:rotate(0)}100%{-webkit-transform:rotate(360deg);-moz-transform:rotate(360deg);-ms-transform:rotate(360deg);-o-transform:rotate(360deg);transform:rotate(360deg)}}@keyframes spinner{0%{-webkit-transform:rotate(0);-moz-transform:rotate(0);-ms-transform:rotate(0);-o-transform:rotate(0);transform:rotate(0)}100%{-webkit-transform:rotate(360deg);-moz-transform:rotate(360deg);-ms-transform:rotate(360deg);-o-transform:rotate(360deg);transform:rotate(360deg)}}</style>
    script(type="text/javascript" src="//platform.linkedin.com/in.js").
      api_key: #{ config.linkedinClientId }
      authorize: true

  body.appear-animate
    //Google Tag Manager (noscript)
    include ga.noscript.pug
    //End Google Tag Manager (noscript)


    div.loader_overlay
      div.loader Loading...

    //Navigation panel
    header.header
      nav.navbar.bg-invere.navbar-dark
        a(href="/").navbar-brand
          img(src=require("images/logo.png") alt="")

        div.navbar-toggleable-md.pull-right
          div#menuProfile

        div.navbar-toggleable-md.pull-right
          ul.nav.navbar-nav#menuNotification

        div.wrap-toggle-menu
          button(type="button" data-toggle="collapse").navbar-toggler.hidden-lg-up#toggle_mobile_menu
            //i.fa.fa-bars
            span

        div.mobile-menu.full-height
          ul.nav.navbar-nav.pull-left#menuList

    div.page#page
      div#content
      footer.footer_new.pt50
      div.hidden

