- pug_html += app.breadcrumbs('Our Partners', '', [ ['Home', '/'], ['Our Partners', ''], ]);

section.text_block.partner-page 
  .container
    .row
      .col-lg-3.hidden-md-down  
        .page_title.border-bottom.mb25
          h4.text-uppercase ABOUT US
        ul.list-unstyled.custom.pages-left-menu
          li
            a(href='/pg/overview').d-inline-block.w-100 GrowthFountain Overview
          li
            a(href='/pg/team').d-inline-block.w-100 #{app.config.teamName}
          li.display-partners.active
            a(href='/pg/partners').d-inline-block.w-100 Partners
          li
            a(href='/pg/contact').d-inline-block.w-100 Contact Us
          li
            a(href='/pg/careers').d-inline-block.w-100 Careers
          li
            a(href='/pg/news').d-inline-block.w-100 In the News
          li
            a(href='/pg/faq').d-inline-block.w-100 FAQ

      .col-lg-9
        .row
          .col-lg-12.text-xs-center
            h2.text-uppercase.m-t-0 PARTNERS
            
        .row
          .col-lg-12
            h3.text-xs-center.m-b-3.font-weight-light.m-t-0 GrowthFountain is proud to partner with companies that share our passion for supporting entrepreneurs, uplifting small businesses and fostering meaningful bonds in local communities.

        h4.section-partner.text-uppercase.section-bg-line.text-xs-center Credit Union Companies
        .row
          .col-sm-4.mb30.text-xs-center
            a(href="http://www.callahan.com/" target="_blank").d-inline-block
