          .col-sm-4.mb30.text-xs-center
            a(href="https://www.rivermarkcu.org/" target="_blank").d-inline-block
              img(src=require("images/partners-logo/river-logo.png")).img-fluid

        .row
          .col-sm-4.m-b-3.text-xs-center
            a(href="https://www.dcu.org/business-banking/business-equity-crowdfunding.html" target="_blank").d-inline-block
              img(src=require("images/partners-logo/dcu-logo.png")).img-fluid
          .col-sm-4.m-b-3.text-xs-center
            a(href="https://www.infinityfcu.com/" target="_blank").d-inline-block
              img(src=require("images/partners-logo/infinityfcu-logo.png")).w-infinityfcu
          .col-sm-4.m-b-3.text-xs-center
            a(href="https://www.tvfcu.com/" target="_blank").d-inline-block
              img(src=require("images/partners-logo/tvfcu.png")).w-tvfcu

        h4.section-partner.text-uppercase.section-bg-line.text-xs-center Incubators
        .row
          .col-lg-4.text-xs-center
            a(href="http://momentum3growth.com/" target="_blank")
              img(src=require("images/partners-logo/momentum-logo.png")).img-fluid.text-lg-right.text-xs-center.m-x-auto
        .row
          .col-xl-12
            hr.m-t-3.m-b-3
        .row
          .col-xl-12
