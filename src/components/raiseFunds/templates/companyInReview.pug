- pug_html += app.breadcrumbs('Congratulations!', '', [ ['Home', '/'], ['Review', '/'], ]);

.container
  .row
    .col-sm-8.offset-sm-2
      h2.text-xs-center.thankyou-title.m0.letter-spacing-normal 
        i(aria-hidden="true").fa.fa-check-square-o.color-green We need to review you campaign!

  .row
    .col-sm-8.offset-sm-2.mb30.mt30
      hr
  
  .row
    .col-sm-12
      .result-info-block.m-x-auto.text-xs-center
        p We need to review you campaign
          br
          |  Thank you for submitting a campaign.
          br
          |  We will review and contact you shortly.
        .info-btn-block.m-t-3
          a(href="/" + company.id).btn.btn-secondary PREVIEW
