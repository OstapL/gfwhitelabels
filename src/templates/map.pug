#map(style="height:400px;width:100%;")
script(type="text/javascript").
  function initMap() {
    var mapElement = document.getElementById('map');
    var uluru = {lat: 40.7440668, lng: -73.98522220000001};
    var map = new google.maps.Map(mapElement, {
      zoom: 15,
      center: uluru,
      scrollwheel: false,
    });
    var marker = new google.maps.Marker({
      position: uluru,
      map: map,
    });
    var infowindow = new google.maps.InfoWindow({
      content:"<b>Growth Fountain</b><br/>79 Madison Ave, 5th Floor, New York, NY 10016<br/> New York"
    });
    google.maps.event.addListener(marker, "click", function(){infowindow.open(map,marker);});
    infowindow.open(map, marker);
  }

if !window.google || !window.google.maps
  script(async defer src="https://maps.googleapis.com/maps/api/js?key=" + googleMapKey + "&callback=initMap")
else
  -
    setTimeout(function() {
      initMap();
    }, 100);