- pug_html += app.breadcrumbs('PARTNERS', '', [ ['Home', '/'], ['Partners', ''], ]);

section.text_block.partner-page 
  .container
    .row
      .col-lg-3.hidden-md-down  
        .page_title.border-bottom.mb25
          h4.text-uppercase ABOUT US
        ul.list-unstyled.custom.pages-left-menu
          li
            a(href='/pg/overview').d-inline-block.w-100 Overview
          li
            a(href='/pg/team').d-inline-block.w-100 #{app.config.teamName}
          li.display-partners.active
            a(href='/pg/partners').d-inline-block.w-100 Partners
          li
            a(href='/pg/contact').d-inline-block.w-100 Contact Us
          li
            a(href='/pg/careers').d-inline-block.w-100 Careers
          li
            a(href='/pg/news').d-inline-block.w-100 In the News
          li
            a(href='/pg/faq').d-inline-block.w-100 FAQ

      .col-lg-9
        .row
          .col-lg-12.text-xs-center
            h2.text-uppercase.m-t-0 OUR PARTNERS
            
        .row
          .col-lg-12
            p.text-xs-center.m-b-3.font-weight-light GrowthFountain is proud to partner with companies that share our passion for supporting entrepreneurs, uplifting small businesses and fostering meaningful bonds in local communities.

        h4.section-partner.text-uppercase.section-bg-line Credit Union Companies
        .row
          .col-sm-4.mt30.text-xs-center
            a(href="http://www.callahan.com/" target="_blank").d-inline-block
              img(src="/img/pages/partners1.png").img-fluid

          .col-sm-4.mt30.text-xs-center
            a(href="https://www.jdcu.org/" target="_blank").d-inline-block
              img(src="/img/pages/partners2.png").img-fluid

          .col-sm-4.mt30.text-xs-center
            a(href="https://www.rivermarkcu.org/" target="_blank").d-inline-block
              img(src="/img/pages/partners3.png").img-fluid

        .row
          .col-sm-4.m-t-2.m-b-3.text-xs-center
            a(href="https://www.dcu.org/" target="_blank").d-inline-block
              img(src="/img/pages/dcu.png").img-fluid

        h4.section-partner.text-uppercase.section-bg-line Incubators
        .row
          .col-lg-4.mt30.text-xs-center
            a(href="http://momentum3growth.com/" target="_blank")
              img(src="/img/momentum3growth.png").img-fluid.text-lg-right.text-xs-center.m-x-auto
  section.home-wrap-text-block.hide-bg-fixed.partners-bgi.m-t-85
